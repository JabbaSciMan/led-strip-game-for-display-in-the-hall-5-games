       basic.pause(100)\n            Strip_A.clear()\n            Strip_A.show()\n            basic.pause(100)\n    if B > Round * Strip_Length - 2:\n        if Red_Won == 0:\n            Green_Won = 1\n            Strip_A.show_color(neopixel.colors(NeoPixelColors.GREEN))\n            basic.pause(100)\n            Strip_A.clear()\n            Strip_A.show()\n            basic.pause(100)\nbasic.forever(on_forever)\n","pxt.json":"{\n    \"name\": \"led strip game\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\",\n        \"neopixel\": \"github:Microsoft/pxt-neopixel#v0.7.3\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"main.py\"\n    ],\n    \"testFiles\": [\n        \"test.ts\"\n    ],\n    \"targetVersions\": {\n        \"branch\": \"v4.0.16\",\n        \"tag\": \"v4.0.16\",\n        \"commits\": \"https://github.com/microsoft/pxt-microbit/commits/04b1a5492fb75af626caed5601ee8d794d970dd7\",\n        \"target\": \"4.0.18\",\n        \"pxt\": \"7.0.12\",\n        \"targetId\": \"microbit\"\n    },\n    \"supportedTargets\": [\n        \"microbit\"\n    ],\n    \"preferredEditor\": \"blocksprj\"\n}\n","test.ts":"// tests go here; this will not be compiled when this package is used as an extension.\n"}},{"timestamp":1768256202952,"editorVersion":"8.0.18","text":{"main.blocks":"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables><variable id=\"NOwU?bpmd.C49z%N9euW\">A</variable><variable id=\"!OVF~gE1xs}[[cr-l*mT\">Round</variable><variable id=\".|MG%$ZNw_Ic5l^*`!:o\">Strip_Length</variable><variable id=\"7g.wFnS]OGqf%ap`@K`@\">Strip_A</variable><variable id=\"/(,.QPY3AT=2)QY,yJ5Z\">B</variable><variable id=\"v@Z9HB:[a=0UuI-,VSYM\">Green_Won</variable><variable id=\"vsC;Loc}o/yJNnJc9`%E\">Red_Won</variable></variables><block type=\"input_logo_event\" x=\"307\" y=\"-134\"><field name=\"action\">TouchButt